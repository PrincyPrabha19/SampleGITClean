                <xs:complexType>
                  <xs:attribute name="Key" type="xs:string" />
                  <xs:attribute name="SubKey" type="xs:string" />
                  <xs:attribute name="Value" type="xs:string" />
                  <xs:attribute name="Executable" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>