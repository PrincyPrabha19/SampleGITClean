<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:Converters="clr-namespace:AlienLabs.AlienAdrenaline.App.Converters" 
	xmlns:Properties="clr-namespace:AlienLabs.AlienAdrenaline.App.Properties" 
	mc:Ignorable="d">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="AlienwareSandbox.xaml"/>
		<ResourceDictionary Source="Brushes.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	<Converters:SoundDeviceStateEnumTranslateConverter x:Key="enumTranslate" />
	<Converters:ConvertBoolToVisibility x:Key="boolToVisibility" />

	<Style BasedOn="{x:Null}" TargetType="{x:Type Button}">	
		<Setter Property="Height" Value="25"/>
		<Setter Property="MinWidth" Value="35"/>
		<Setter Property="MinHeight" Value="25"/>
		<Setter Property="Foreground" Value="#FF333333"/>
		<Setter Property="FontFamily" Value="Pill Gothic 600mg Semibd"/>
		<Setter Property="FontSize" Value="12.667"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Button}">
					<Grid x:Name="grid">
						<Rectangle x:Name="buttonBase" Stretch="Fill" StrokeLineJoin="Round" RadiusX="2" RadiusY="2" Fill="{DynamicResource Fill_Button}" Margin="0" Stroke="{DynamicResource Stroke_Button}" StrokeThickness="1"/>
						<ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" Margin="16,3,16,0" Content="{TemplateBinding Content}"/>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="StrokeThickness" TargetName="buttonBase" Value="1.8"/>
							<Setter Property="Stroke" TargetName="buttonBase" Value="{DynamicResource AlienBlue}"/>	
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Stroke" TargetName="buttonBase" Value="{DynamicResource Stroke_Button}"/>
							<Setter Property="Foreground" Value="#FF333333"/>
							<Setter Property="Fill" TargetName="buttonBase" Value="{DynamicResource Fill_ButtonPressed}"/>
							<Setter Property="StrokeThickness" TargetName="buttonBase" Value="1"/>	
						</Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="#FF939393" />
                        </Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

    <Style x:Key="DeleteButtonStyle" BasedOn="{x:Null}" TargetType="{x:Type Button}">
        <Setter Property="Height" Value="25"/>
        <Setter Property="MinWidth" Value="30"/>
        <Setter Property="MinHeight" Value="25"/>
        <Setter Property="Foreground" Value="#FF333333"/>
        <Setter Property="FontFamily" Value="Pill Gothic 600mg Semibd"/>
        <Setter Property="FontSize" Value="12.667"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="grid">
                        <Border BorderThickness="1,0,0,0" BorderBrush="Gray" />
                        <Image x:Name="image2" Source="/AlienAdrenaline;component/Media/Buttons/delete.png" Stretch="Fill" Width="18" Height="19" />
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0" Content="{TemplateBinding Content}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ActionButtonStyle" BasedOn="{x:Null}" TargetType="{x:Type ToggleButton}">
        <Setter Property="Height" Value="25"/>
        <Setter Property="MinWidth" Value="35"/>
        <Setter Property="MinHeight" Value="25"/>
        <Setter Property="Foreground" Value="#FF333333"/>
        <Setter Property="FontFamily" Value="Pill Gothic 600mg Semibd"/>
        <Setter Property="FontSize" Value="12.667"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Grid x:Name="grid">
                        <Border x:Name="border" BorderThickness="1,1,1,1" CornerRadius="4" Background="{DynamicResource Fill_AppBlackBtt}" BorderBrush="{DynamicResource Stroke_NavigationBorder}" Grid.Column="1" Height="Auto" Margin="0,5" Width="{TemplateBinding Width}"/>
                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="Center" Margin="14,0,0,0" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True" SourceName="grid">
                            <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource Stroke_NavigationBorderHover}" />
                            <Setter Property="Effect" TargetName="border">
                                <Setter.Value>
                                    <DropShadowEffect BlurRadius="12" Color="#FF00d8ff" Opacity="1" ShadowDepth="0" />
                                </Setter.Value>
                            </Setter>                            
                        </Trigger>
                        <Trigger Property="IsChecked" Value="true">
                            <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource Stroke_NavigationBorderChecked}" />
                            <Setter Property="Background" TargetName="border" Value="{DynamicResource Fill_NavigationBackgroundChecked}" />
                            <Setter Property="BorderThickness" TargetName="border" Value="2" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Background" TargetName="border" Value="{DynamicResource Fill_NavigationBackgrounDisabled}" />
                            <Setter Property="BorderBrush" TargetName="border" Value="Black" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="SoundDeviceRadioButtonStyle" BasedOn="{x:Null}" TargetType="{x:Type RadioButton}">
        <Setter Property="MinWidth" Value="35"/>
        <Setter Property="MinHeight" Value="25"/>
        <Setter Property="Foreground" Value="#FF333333"/>
        <Setter Property="FontFamily" Value="Pill Gothic 600mg Semibd"/>
        <Setter Property="FontSize" Value="12.667"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <Grid x:Name="grid" Margin="0" HorizontalAlignment="Stretch">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" MinWidth="10"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>

                        <Border x:Name="border" BorderThickness="1,1,1,1" CornerRadius="4" Grid.ColumnSpan="2" Height="Auto" HorizontalAlignment="Stretch" Margin="0"/>
                        <Image x:Name="imageDevice" Width="48" Height="48" Source="{Binding Image}" Margin="10,0,10,0" IsEnabled="{Binding IsEnabled}">
                            <Image.Style>
                                <Style TargetType="Image">
                                    <Style.Triggers>
                                        <Trigger Property="IsEnabled" Value="False">
                                            <Setter Property="Opacity" Value="0.5" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Image.Style>
                        </Image>
                        <Image x:Name="imageDeviceStatus" Width="24" Height="24" HorizontalAlignment="Right" VerticalAlignment="Bottom" d:LayoutOverrides="Width, Height, Margin" />
                        <StackPanel Orientation="Vertical" Grid.Column="1">
                        	<StackPanel Orientation="Horizontal">
                        		<TextBlock x:Name="textBlockDeviceName" Text="{Binding Name}" VerticalAlignment="Stretch" d:LayoutOverrides="Height" Margin="15,15,0,0" FontSize="12" HorizontalAlignment="Left" />							               	
							</StackPanel>                            
                            <TextBlock x:Name="textBlockDeviceState" Text="{Binding Path=State, Mode=OneWay, Converter={StaticResource enumTranslate}}" VerticalAlignment="Stretch" d:LayoutOverrides="Height" Margin="15,5,0,0" FontSize="12" HorizontalAlignment="Left" Foreground="#FF999999" />
                        </StackPanel>                        
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True" SourceName="grid">
                            <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource AlienBlue}" />
                            <Setter Property="Background" TargetName="border">
                                <Setter.Value>
                                    <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                        <LinearGradientBrush.GradientStops>
                                            <GradientStop Color="#FF373737"/>
                                            <GradientStop Color="#FF1A1A1A" Offset="1"/>
                                        </LinearGradientBrush.GradientStops>
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <!--
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Source" TargetName="imageDeviceStatus" Value="/AlienAdrenaline;component/Media/Buttons/audiodevice_notpresent.png" />
                        </Trigger> 
                        -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsChecked" Value="true" />
                            </MultiTrigger.Conditions>
                            <Setter Property="Source" TargetName="imageDeviceStatus" Value="/AlienAdrenaline;component/Media/Buttons/audiodevice_selected.png" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

