                        	</Label>
                        </StackPanel>
                        <StackPanel x:Name="stackPlay" Orientation="Horizontal" d:LayoutOverrides="Width" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,3,32,0">
                        	<Image x:Name="imgPlay" HorizontalAlignment="Stretch" Source="/AlienAdrenaline;component/Media/play.png" Stretch="Fill" Width="24" VerticalAlignment="Center" Height="24"/>
                        	<Label x:Name="labelPlay" Content="{x:Static Properties:Resources.CompareText}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  Height="28" FontSize="14.667" FontFamily="Pill Gothic 600mg Light" Width="Auto" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource AlienBlue}" MaxWidth="92" Style="{DynamicResource LabelWithEllipsisStyle}">
                        		<i:Interaction.Behaviors>
                        			<AlienLabs_Tools_Behaviors:CaseBehavior/>
                        		</i:Interaction.Behaviors>
                        	</Label>
                        </StackPanel>
                        <Path Fill="#00000000" Stretch="Fill" Stroke="#00000000" StrokeThickness="1" Data="M43.535104,0.5 L118.5,0.5 118.5,34.5 0.5,34.5 C20.497189,17.765173 32.843475,-1.0723955 43.535104,0.5 z" RenderTransformOrigin="0.5,0.5" x:Name="pathMouseOverZone" Width="164" Height="34.092">
                            <Path.BitmapEffect>
                                <BevelBitmapEffect BevelWidth="0" EdgeProfile="CurvedIn"/>
                            </Path.BitmapEffect>
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="-1" ScaleY="1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="0"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" TargetName="labelPlay" Value="#FF787878"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True" SourceName="pathMouseOverZone">
                            <Setter Property="Visibility" TargetName="pathBlueShadow" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


</ResourceDictionary>