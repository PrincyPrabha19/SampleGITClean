										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="stackPlay">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{x:Static Visibility.Visible}"/>
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>
						</VisualStateManager.VisualStateGroups>	
						
                        <Path Fill="{DynamicResource Fill_AppBlackBtt}" Stretch="Fill" Stroke="{DynamicResource Stroke_AppBtt}" StrokeThickness="1" Data="M43.535104,0.5 L118.5,0.5 118.5,34.5 0.5,34.5 C20.497189,17.765173 32.843475,-1.0723955 43.535104,0.5 z" RenderTransformOrigin="0.5,0.5" Margin="0,0,1,0" x:Name="pathButton" Width="164" Height="34.092">
                            <Path.BitmapEffect>
                                <BevelBitmapEffect BevelWidth="0" EdgeProfile="CurvedIn"/>
                            </Path.BitmapEffect>
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="-1" ScaleY="1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="0"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                        <Path Fill="{DynamicResource Fill_AppBlackBttOver}" Stretch="Fill" StrokeThickness="1" Data="M43.535104,0.5 L118.5,0.5 118.5,34.5 0.5,34.5 C20.497189,17.765173 32.843475,-1.0723955 43.535104,0.5 z" RenderTransformOrigin="0.5,0.5" Visibility="Hidden" Stroke="{DynamicResource Stroke_AppBtt}" x:Name="pathBlueShadow" Width="164" Height="34.092">
                            <Path.BitmapEffect>
                                <BevelBitmapEffect BevelWidth="0" EdgeProfile="CurvedIn"/>
                            </Path.BitmapEffect>
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="-1" ScaleY="1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="0"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                        <StackPanel x:Name="stackPause" Orientation="Horizontal" d:LayoutOverrides="Width" HorizontalAlignment="Center" Margin="0,3,32,0" Visibility="Hidden" VerticalAlignment="Center">
                        	<Image x:Name="imgPause" HorizontalAlignment="Stretch" Source="/AlienAdrenaline;component/Media/pause.png" Stretch="Fill" Width="24" VerticalAlignment="Center" Height="24"/>
                        	<Label x:Name="labelPause" Content="{x:Static Properties:Resources.PauseText}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  Height="28" FontSize="14.667" FontFamily="Pill Gothic 600mg Light" Width="Auto" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource AlienBlue}" MaxWidth="92" Style="{DynamicResource LabelWithEllipsisStyle}">
                        		<i:Interaction.Behaviors>
                        			<AlienLabs_Tools_Behaviors:CaseBehavior/>
