	<Style x:Key="navigationRBT" TargetType="{x:Type RadioButton}">
		<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
		<Setter Property="Background" Value="#F4F4F4"/>
		<Setter Property="BorderBrush" Value="{StaticResource CheckBoxStroke}"/>
		<Setter Property="BorderThickness" Value="1"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type RadioButton}">
					<Grid x:Name="grid">
						<Rectangle Fill="#FF000000" Stroke="{DynamicResource greyBrush}" x:Name="rectangle"/>
						<Path Fill="{DynamicResource greyBrush}" Stretch="Fill" Stroke="{x:Null}" x:Name="path" Width="7.51" Data="M244.02721,27.761522 L244.02721,40.842244 237.51566,34.741402 z" HorizontalAlignment="Right" VerticalAlignment="Center" Height="14.078"/>
						<Label HorizontalAlignment="Left" x:Name="label" VerticalAlignment="Center" FontFamily="{DynamicResource DefaultFont}" FontSize="{TemplateBinding FontSize}" FontStyle="{TemplateBinding FontStyle}" Foreground="{TemplateBinding Foreground}" Margin="5,1,5,0" Content="{TemplateBinding Content}"/>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsChecked" Value="True">
							<Setter Property="BitmapEffect" TargetName="rectangle" Value="{DynamicResource AlienBlueGlow}"/>
							<Setter Property="BitmapEffect" TargetName="path" Value="{DynamicResource AlienBlueGlow}"/>
							<Setter Property="Foreground" TargetName="label" Value="#FFFFFFFF"/>
							<Setter Property="Fill" TargetName="path" Value="{DynamicResource AlienBlueBrush}"/>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource AlienBlueBrush}"/>
						</Trigger>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="Foreground" TargetName="label" Value="#FFFFFFFF"/>
							<Setter Property="BitmapEffect" TargetName="rectangle" Value="{DynamicResource AlienBlueGlow}"/>
							<Setter Property="BitmapEffect" TargetName="path" Value="{DynamicResource AlienBlueGlow}"/>
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource LeftBlue}"/>
							<Setter Property="Fill" TargetName="path" Value="{DynamicResource AlienBlueBrush}"/>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource AlienBlueBrush}"/>
						</Trigger>
						<Trigger Property="HasContent" Value="true">
							<Setter Property="FocusVisualStyle" Value="{StaticResource CheckRadioFocusVisual}"/>
							<Setter Property="Padding" Value="4,0,0,0"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="false">
							<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	<Style x:Key="ButtonFocusVisual">
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate>
					<Rectangle SnapsToDevicePixels="true" Stroke="Black" StrokeDashArray="1 2" StrokeThickness="1" Margin="2"/>
